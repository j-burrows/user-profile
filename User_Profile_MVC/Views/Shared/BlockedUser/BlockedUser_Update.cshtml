@model UserProfileLib.Business.BBlocked_User
@using Repository.Business.Protocols
@using Mailbox.Helpers

@{Model.Format();}

@using (Html.BeginForm("BlockedUser_Update", "Home", FormMethod.Post, 
    new {
        @id = "BlockedUserUpdate_" + Model.Blocked_User_ID,
        @class = "BlockedUserUpdate PartialUpdateForm"
    })) { 
    @Model.Blocked_User_ID_Rules.Update.HtmlInput(Model.Blocked_User_ID)
    @Model.username_Rules.Update.HtmlInput(Model.username)
    @Model.Author_Name_Rules.Update.HtmlInput(Model.Author_Name)
    
    <input type="submit" value="Update Blocked User" />
}